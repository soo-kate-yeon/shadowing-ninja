      },
      "fadeOut": {
        "from": {
          "opacity": "1"
        },
        "to": {
          "opacity": "0"
        }
      },
      "scaleIn": {
        "from": {
          "opacity": "0",
          "transform": "scale(0.95)"
        },
        "to": {
          "opacity": "1",
          "transform": "scale(1)"
        }
      },
      "slideUp": {
        "from": {
          "opacity": "0",
          "transform": "translateY(10px)"
        },
        "to": {
          "opacity": "1",
          "transform": "translateY(0)"
        }
      },
      "slideDown": {
        "from": {
          "opacity": "0",
